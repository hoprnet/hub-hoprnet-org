{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
            {
                "key": "X-Frame-Options",
                "value": "DENY"
            }
            ]
        }
    ],
    "contentSecurityPolicy": "frame-src 'none';"
}